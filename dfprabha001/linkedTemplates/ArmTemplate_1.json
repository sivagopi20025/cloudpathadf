{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfprabha001"
		},
		"delete_schedule_properties_Delete activity_parameters_sinkfolder": {
			"type": "string",
			"defaultValue": "employ"
		},
		"delete_schedule_properties_getmetadata activity_parameters_foldername": {
			"type": "string",
			"defaultValue": "sample"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/delete_schedule')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Started",
				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "Delete activity",
							"type": "PipelineReference"
						},
						"parameters": {
							"sinkfolder": "[parameters('delete_schedule_properties_Delete activity_parameters_sinkfolder')]"
						}
					},
					{
						"pipelineReference": {
							"referenceName": "getmetadata activity",
							"type": "PipelineReference"
						},
						"parameters": {
							"foldername": "[parameters('delete_schedule_properties_getmetadata activity_parameters_foldername')]"
						}
					}
				],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "Hour",
						"interval": 1,
						"startTime": "2024-05-10T10:30:00",
						"endTime": "2024-05-10T10:45:00",
						"timeZone": "Central Standard Time"
					}
				}
			},
			"dependsOn": []
		}
	]
}