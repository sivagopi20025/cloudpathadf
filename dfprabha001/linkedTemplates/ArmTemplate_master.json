{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfprabha001"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"ls_ADLS_bgcsrk001_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_ADLS_bgcsrk001'"
		},
		"ls_Asql_bgcsrk001_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_Asql_bgcsrk001'"
		},
		"ls_prabhaadls001_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_prabhaadls001'"
		},
		"ls_prabhablob001_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_prabhablob001'"
		},
		"ls_ADLS_bgcsrk001_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsbgcsr001.dfs.core.windows.net/"
		},
		"ls_prabhaadls001_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsprabha001.dfs.core.windows.net/"
		},
		"ls_restapiemp_bgcsrk001_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dummy.restapiexample.com/api/v1/employees"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dfprabha001_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"ls_ADLS_bgcsrk001_accountKey": {
						"value": "[parameters('ls_ADLS_bgcsrk001_accountKey')]"
					},
					"ls_Asql_bgcsrk001_connectionString": {
						"value": "[parameters('ls_Asql_bgcsrk001_connectionString')]"
					},
					"ls_prabhaadls001_accountKey": {
						"value": "[parameters('ls_prabhaadls001_accountKey')]"
					},
					"ls_prabhablob001_connectionString": {
						"value": "[parameters('ls_prabhablob001_connectionString')]"
					},
					"ls_ADLS_bgcsrk001_properties_typeProperties_url": {
						"value": "[parameters('ls_ADLS_bgcsrk001_properties_typeProperties_url')]"
					},
					"ls_prabhaadls001_properties_typeProperties_url": {
						"value": "[parameters('ls_prabhaadls001_properties_typeProperties_url')]"
					},
					"ls_restapiemp_bgcsrk001_properties_typeProperties_url": {
						"value": "[parameters('ls_restapiemp_bgcsrk001_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}